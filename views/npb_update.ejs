<!Doctype html>
<html>
<head>
<meta charset="utf-8">
<title>チーム編集</title>
</head>
<body>
<h2><%= mes %></h2>
<% for( let row of data ) { %>
<form action="/update" method="post">
<input type="number" name="id" value="<%= row.team_id %>">
<input type="text" name="team" value="<%= row.チーム名 %>">
<input type="text" name="where" value="<%= row.所在地 %>">
<input type="text" name="stadium" value="<%= row.本拠地 %>">
<input type="submit" value="登録">
</form>
<% } %>
</body>
</html>
